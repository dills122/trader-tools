{
  "name": "cron-app",
  "version": "0.2.12",
  "description": "runs all trading cron jobs",
  "main": "cron-runner.js",
  "scripts": {
    "test": "echo \"Error: no test yet\" && exit 0",
    "build": "tsc -p .",
    "compile": "tsc",
    "clean": "tsc --build --clean && rm -rf ./types",
    "start": "node cron-runner.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "pm2": "pm2 start processes.json --no-daemon",
    "pm2:dev": "ts-node-dev --respawn --transpile-only ./cron-runner.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "trading-services": "workspace:^0.2.12",
    "node-cron": "~3.0.0",
    "express": "~4.17.1",
    "pm2": "^5.1.0"
  },
  "devDependencies": {
    "@types/node-cron": "~2.0.3",
    "@types/express": "~4.17.11",
    "@typescript-eslint/eslint-plugin": "~5.12.0",
    "@typescript-eslint/parser": "~5.12.0",
    "eslint": "~8.9.0",
    "typescript": "4.5.5",
    "ts-node-dev": "^1.1.8"
  },
  "engines": {
    "node": "16.x"
  }
}

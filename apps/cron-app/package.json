{
  "name": "cron-app",
  "version": "0.2.12",
  "description": "runs all trading cron jobs",
  "main": "cron-runner.js",
  "scripts": {
    "test": "echo \"Error: no test yet\" && exit 0",
    "build": "tsc -p .",
    "compile": "tsc",
    "clean": "tsc --build --clean && rm -rf ./types",
    "start": "node cron-runner.js",
    "start:dev": "ts-node-dev --respawn --transpile-only ./cron-runner.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "pm2": "pm2 start processes.json --no-daemon"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "~16.0.0",
    "trading-services": "workspace:^0.2.12",
    "node-cron": "~3.0.0",
    "express": "~4.18.0",
    "pm2": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "~18.11.9",
    "@types/node-cron": "~3.0.6",
    "@types/express": "~4.17.11",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "eslint": "~8.29.0",
    "typescript": "~4.9.3",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": "16.x"
  }
}
